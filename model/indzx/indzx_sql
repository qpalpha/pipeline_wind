SELECT
WINDDF.ASHAREINDUSTRIESCLASSCITICS.WIND_CODE AS TICKER,
WINDDF.ASHAREINDUSTRIESCLASSCITICS.CITICS_IND_CODE AS ZX_CODE,
WINDDF.ASHAREINDUSTRIESCLASSCITICS.ENTRY_DT AS BEGINDATE,
WINDDF.ASHAREINDUSTRIESCLASSCITICS.REMOVE_DT AS ENDDATE
FROM
WINDDF.ASHAREINDUSTRIESCLASSCITICS
ORDER BY
BEGINDATE ASC